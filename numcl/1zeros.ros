(defpackage :ros.script.benchmark
  (:use :numcl :trivial-benchmark))
(in-package :ros.script.benchmark)
#+sbcl
(declaim (sb-ext:muffle-conditions sb-ext:compiler-note style-warning))

(defun main ()
  (with-timing (1000)
    (let ((a (zeros 1000000 :type 'single-float)))
      (setf (aref a 0) 1.0
            (aref a -1) 1.0))))
